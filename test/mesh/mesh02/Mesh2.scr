// Families: y = 2, or y > 2 -- x doesn't matter
global protocol Proto3(role W)
{
	foreach W[I1:(1,1),K1h-(0,1)], W[I2:(1,2),K1h]
	{
		Foo(string) from W[I1] to W[I2];
	}
}

