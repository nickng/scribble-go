// http://sandbox.kidstrythisathome.com/erdos/


module Foreach11;


sig <go> "messages.Foo" from "github.com/rhu1/scribble-go-runtime/test/foreach/foreach11/messages" as Foo;
sig <go> "messages.Bar" from "github.com/rhu1/scribble-go-runtime/test/foreach/foreach11/messages" as Bar;


// Families: K=3 and K>3
global protocol Proto1(role W)
{
	choice at W[1]
	{
		Foo from W[1] to W[2];
		foreach W[I:2,K-1], W[J:3,K]
		{
			Foo from W[I] to W[J];
		}
		Foo from W[K] to W[1];
		//do Proto1(W);
	}
	or
	{
		Bar from W[1] to W[2];
		foreach W[I:2,K-1], W[J:3,K]
		{
			Bar from W[I] to W[J];
		}
		Bar from W[K] to W[1];
	}
}

