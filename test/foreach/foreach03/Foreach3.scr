// http://sandbox.kidstrythisathome.com/erdos/


module Foreach3;


type <go> "int" from "..." as int;


// i.e., All-to-All -- A(int) from S[1,K1] to W[1.K2]
global protocol Proto1(role S, role W)
{
	foreach S[I1:1,K1]
	{
		foreach W[I2:1,K2]
		{
			A(int) from S[I1,I1] to W[I2,I2];
		}
	}	
}
